{
  "name": "@superstream/inspector",
  "version": "0.0.7",
  "author": "Garvit Pahal <g10pahal@gmail.com>",
  "repository": "https://github.com/superstream-finance/superstream",
  "bugs": "https://github.com/superstream-finance/superstream/issues",
  "homepage": "https://superstream.finance/",
  "license": "MIT",
  "keywords": [
    "superstream",
    "solana",
    "payment",
    "stream",
    "inspector",
    "solvent",
    "insolvent",
    "typescript",
    "javascript"
  ],
  "engines": {
    "node": ">=14"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "inspector": "./bin/run"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "oclif": {
    "bin": "inspector",
    "dirname": "inspector",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-warn-if-update-available"
    ],
    "topicSeparator": ":",
    "hooks": {
      "command_not_found": "./dist/hooks/command_not_found"
    }
  },
  "scripts": {
    "clean": "rimraf dist/",
    "generate": "pnpm build && run-p generate:**",
    "generate:manifest": "oclif manifest",
    "generate:docs": "ts-node scripts/generate-docs.ts",
    "prepack": "pnpm generate",
    "postpack": "rimraf oclif.manifest.json",
    "build": "pnpm clean && tsc -b",
    "start": "./bin/run",
    "dev": "./bin/dev",
    "lint": "eslint \"**/*.ts\"",
    "fix": "eslint \"**/*.ts\" --fix"
  },
  "dependencies": {
    "@oclif/core": "^1.16.0",
    "@oclif/plugin-warn-if-update-available": "^2.0.4",
    "@project-serum/anchor": "^0.25.0",
    "@superstream/client": "^0.7.2",
    "clean-stack": "^4.2.0",
    "fast-levenshtein": "^3.0.0",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@types/fast-levenshtein": "^0.0.2",
    "@types/fs-extra": "^9.0.13",
    "@types/github-slugger": "^1.3.0",
    "@types/node": "^18.7.14",
    "eslint": "^8.23.0",
    "fs-extra": "^10.1.0",
    "github-slugger": "^1.4.0",
    "globby": "^11.1.0",
    "npm-run-all": "^4.1.5",
    "oclif": "^3.1.2",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.2"
  }
}
